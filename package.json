{
  "name": "@source4society/react-scepter-web-components",
  "version": "1.1.0",
  "description": "A set of common components implemented on web-based react, useful for injecting into a wrapper for platform independent implementations.",
  "main": "lib/View.js",
  "repository": "https://github.com/source4societyorg/react-scepter-web-components.git",
  "author": "Source 4 Society",
  "license": "AGPL-3.0",
  "private": false,
  "scripts": {
    "test": "eslint ./src/*.js ./tests/*.js && jest --coverage",
    "build": "babel src/View.js --minified --out-file lib/View.js && babel src/ActivityIndicator.js --minified --out-file lib/ActivityIndicator.js && babel src/Button.js --minified --out-file lib/Button.js && babel src/Div.js --minified --out-file lib/Div.js && babel src/Header.js --minified --out-file lib/Header.js && babel src/Image.js --minified --out-file lib/Image.js && babel src/Paragraph.js --minified --out-file lib/Paragraph.js",
    "prepublish": "npm run build"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "html",
      "cobertura"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "coverageDirectory": "./coverage",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 99,
        "branches": 99,
        "functions": 99,
        "lines": 99
      }
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test-bundler.js",
    "testRegex": "tests/.*\\.test\\.js$"
  },
  "dependencies": {
    "@source4society/react-loadingindicator-component": "^1.0.0",
    "@source4society/scepter-utility-lib": "^1.1.0",
    "immutable": "3.8.1",
    "moment": "^2.20.1",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-router-dom": "4.1.1",
    "react-test-renderer": "15.6.1",
    "redux-saga": "^0.16.0",
    "reselect": "3.0.1",
    "styled-components": "^3.2.5"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "^22.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dynamic-import-node": "1.0.2",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-styled-components": "1.1.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.5",
    "babel-preset-env": "1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-redux-saga": "^0.6.0",
    "jest-cli": "21.0.0"
  }
}
